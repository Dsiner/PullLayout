<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="lib_pull_PullLayout">
        <attr name="lib_pull_enable" format="boolean" />
        <attr name="lib_pull_gravity">
            <flag name="top" value="0x0001" />
            <flag name="bottom" value="0x0010" />
            <flag name="left" value="0x0100" />
            <flag name="right" value="0x1000" />
        </attr>
    </declare-styleable>

    <declare-styleable name="lib_pull_PullRecyclerLayout">
        <attr name="lib_pull_type">
            <flag name="list_view" value="1" />
            <flag name="recycler_view" value="2" />
            <flag name="pull_recycler_view" value="3" />
        </attr>
    </declare-styleable>
</resources>
