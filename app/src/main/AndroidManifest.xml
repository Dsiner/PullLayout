<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.d.pulllayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/lib_pub_AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".edge.RippleActivity" />
        <activity android:name=".pull.activity.PullActivity" />
        <activity android:name=".pull.activity.PullViewActivity" />
        <activity android:name=".pull.activity.PullListViewActivity" />
        <activity android:name=".pull.activity.PullRecyclerViewActivity" />
        <activity android:name=".pull.activity.PullScrollViewActivity" />

        <activity android:name=".list.activity.ListActivity" />
        <activity android:name=".list.activity.CoordinatorTabActivity" />
    </application>

</manifest>