<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.d.pulllayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.d.pulllayout.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.d.pulllayout.edge.RippleActivity" />
        <activity android:name="com.d.pulllayout.pull.activity.ViewActivity" />
        <activity android:name="com.d.pulllayout.pull.activity.ListViewActivity" />
        <activity android:name="com.d.pulllayout.pull.activity.RecyclerViewActivity" />
        <activity android:name="com.d.pulllayout.pull.activity.ScrollViewActivity" />
        <activity android:name="com.d.pulllayout.pull.activity.PullRecyclerLayoutActivity" />

        <activity android:name="com.d.pulllayout.rv.activity.SimpleActivity" />
        <activity android:name="com.d.pulllayout.rv.activity.MultipleActivity" />
        <activity android:name="com.d.pulllayout.rv.activity.ItemTouchActivity" />
    </application>

</manifest>